package com.kiran.arrays;

import java.util.Arrays;

public class ArrayCreation {
	public static void main(String[] args) {
		
		int[] intArr = new int[5];
		
		intArr[0] = 28;
		intArr[1] = 23;
		intArr[4] = 13;
		
		System.out.println(intArr);
		
		System.out.println(Arrays.toString(intArr));
	}
}
