package com.kiran.basics;

public class SumTwoNumbers {
	private static int add(int int1, int int2) {
		int sum = int1 + int2;
		return sum;
	}
	
	public static void main(String[] args) {
		
		int firstInt = 7;
		int secondInt = 8;
		
		int sum = add(firstInt, secondInt);
		
		System.out.println("The sum is: " + sum);
	}
}
